<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="300dip" />
            <stroke android:width="1dip" android:color="#49bd22" />
            <gradient android:angle="-90" android:startColor="#52d625" android:endColor="#54db16"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="30dip" />
            <stroke android:width="1dip" android:color="#52d625" />
            <solid android:color="#55de26"/>
        </shape>
    </item>  
    <item >
       <shape android:shape="rectangle"  >
            <corners android:radius="300dip" />
            <stroke android:width="1dip" android:color="#49bd22" />
            <gradient android:angle="-90" android:startColor="#52d625" android:endColor="#54db16" />
       </shape>
    </item>
</selector>
